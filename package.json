{
  "name": "ngm",
  "private": true,
  "scripts": {
    "lint": "eslint src/ test/",
    "extract-from-assets": "node -r esm scripts/get_assets_properties.js > src/assets_properties.js",
    "extract-i18n": "i18next",
    "start": "npm run webpack-start",
    "webpack-start": "node scripts/environment.js && node scripts/generateManuals.js && webpack-dev-server",
    "rollup-start": "node scripts/environment.js && node scripts/generateManuals.js && rollup --environment SERVE:true -c --watch --no-treeshake",
    "build": "node scripts/environment.js $RELEASE_NAME && node scripts/generateManuals.js && rm -rf dist/* && rollup --environment mode:production -c && scripts/create_versions.sh",
    "test": "mocha --exit"
  },
  "dependencies": {
    "@geoblocks/cesium-compass": "0.3.0",
    "@geoblocks/cesium-view-cube": "0.0.1",
    "@geoblocks/ga-search": "^0.0.18",
    "@sentry/browser": "^5.22.3",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "cesium": "1.72.0",
    "event-target-shim": "^5.0.1",
    "fomantic-ui-css": "^2.8.7",
    "i18next": "^19.7.0",
    "i18next-xhr-backend": "^3.2.2",
    "interactjs": "^1.9.22",
    "jquery": "^3.5.1",
    "lit-html": "^1.3.0",
    "loc-i18next": "^0.1.4",
    "proj4": "^2.6.2",
    "typeface-source-sans-pro": "1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-commonjs": "^15.0.0",
    "autoprefixer": "^9.8.6",
    "copy-webpack-plugin": "^6.0.4",
    "core-js": "^3.6.5",
    "css-loader": "^4.2.2",
    "eslint": "^7.8.0",
    "esm": "^3.2.25",
    "file-loader": "^6.1.0",
    "i18next-parser": "^3.3.0",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^0.11.0",
    "mocha": "^8.1.3",
    "node-fetch": "^2.6.0",
    "postcss-import": "^12.0.1",
    "postcss-inline-svg": "^4.1.0",
    "postcss-url": "^8.0.0",
    "rollup": "^2.26.9",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-ejs": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-strip-pragma": "^1.0.0",
    "rollup-plugin-terser": "^7.0.1",
    "source-map-loader": "^1.1.0",
    "speed-measure-webpack-plugin": "^1.3.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
